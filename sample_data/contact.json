{
  "firstName": "$first",
  "lastName": "$last",
  "fullName": "$name",
  "preferredName": "$first",
  "role": "$profession",
  "social": {
    "linkedInUrl": "$url",
    "twitter": "$url"
  },
  "email": "$email",
  "phones": {
    "$array": {"of": {
        "type": {"$choose": {"from": ["cell","home","office"]}},
        "phoneNumber": "$phone"
      },
      "number": 3
    }
  },
  "address": {
    "city": "$city",
    "state": "$state",
    "nearestMajorCity": "$city",
    "location": "$coordinates"
  },
  "account": {
    "name": "$company",
    "_id": "$oid",
    "salesForceId": "$url"
  },
  "firstContacted": {"$date": {"min": "2020-01-01",
    "max": "2022-12-31"
  }},
  "lastContacted": {"$date": {"min": "2020-01-01",
    "max": "2022-12-31"
  }}
}
